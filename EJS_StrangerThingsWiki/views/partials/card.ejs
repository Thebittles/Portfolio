        <% const {name, img_url, status} = data %> 
        <!-- Card -->
        <div class="card p-2 mx-2 my-3" style="width: 18rem;">
            <img class="card-img-top" src="<%=img_url%>" alt="Card image cap">
                <div class="card-body">
                    <p class="card-text text-center">
                        <%=name%>
                    </p>
                    <p class="card-text text-center">Status: 
                        <%=status%>
                </p>
            </div>
        </div>